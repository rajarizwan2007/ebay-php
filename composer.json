{
    "name": "rajarizwan2007/ebay-php",
    "description": "Updated version of dts/ebay-sdk-php package.",
    "keywords": ["ebay", "php", "api", "sdk"],
    "homepage": "http://devbay.net/",
    "type": "library",
    "license": "(Apache-2.0 and MIT)",
    "authors": [
        {
            "name": "Raja Rizwan",
            "email": "rajarizwan2007@googlemail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/rajarizwan2007/ebay-php/issues"
    },
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "~7.2",
        "guzzlehttp/psr7": "~2.0",
        "guzzlehttp/promises": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.1",
        "squizlabs/php_codesniffer": "4.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "DTS\\eBaySDK\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "DTS\\eBaySDK\\Test\\": "test/"
        },
        "classmap": ["build/"]
    }
}
